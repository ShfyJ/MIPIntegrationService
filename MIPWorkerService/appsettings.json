{
  "JwtSettings": {
    "Issuer": "ASKUG.UNG",
    "Audience": "MIPPushService",
    "ExpiresInMinutes": 60
  },
  "ExternalService": {
    "PostDataUrl": "https://pushservice.egov.uz/v3/app/mq/receive",
    "FetchSubscribersUrl": "https://pushservice.egov.uz/v3/app/mq/publisher/fetch-subscribers",
    "FetchDeliverySummaryUrl": "https://pushservice.egov.uz/v3/app/mq/publisher/fetch-daily-delivery-statistics",
    "FetchReportUrl": "https://pushservice.egov.uz/v3/app/mq/publisher/fetch-delivery-detailed-report/"
  },
  "SourceDatabase": {
    "ConnectionString": "Data Source=MSPD-PROKUR;Database=asodu;Trusted_Connection=True;Encrypt=True;TrustServerCertificate=True;"
  },
  "TargetDatabase": {
    "ConnectionString": "Server=localhost;Database=MIPDb;Trusted_Connection=True;Encrypt=True;TrustServerCertificate=True;"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log-.txt",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "MIPWorkerService"
    }
  }
}
